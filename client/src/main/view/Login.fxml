<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="285.0" prefWidth="500.0" GridPane.columnIndex="1" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.main.view.LoginController">
    <children>
        <Button fx:id="loginBtn" layoutX="100.0" layoutY="200.0" mnemonicParsing="false" onAction="#login" prefHeight="40.0" prefWidth="95.0" text="登陆">
            <font>
                <Font size="20.0" />
            </font></Button>
        <Button fx:id="registerBtn" layoutX="305.0" layoutY="200.0" mnemonicParsing="false" onAction="#signIn" prefHeight="40.0" prefWidth="95.0" text="注册">
            <font>
                <Font size="20.0" />
            </font></Button>
        <TextField fx:id="nameField" layoutX="195.0" layoutY="56.0" prefHeight="40.0" prefWidth="240.0" />
        <TextField fx:id="passwordField" layoutX="195.0" layoutY="128.0" prefHeight="40.0" prefWidth="240.0" />
        <Label layoutX="100.0" layoutY="56.0" prefHeight="40.0" prefWidth="82.0" text="用户名">
            <font>
                <Font size="25.0" />
            </font></Label>
        <Label layoutX="100.0" layoutY="128.0" prefHeight="40.0" prefWidth="82.0" text="密   码">
            <font>
                <Font size="25.0" />
            </font></Label>
    </children>
</AnchorPane>
